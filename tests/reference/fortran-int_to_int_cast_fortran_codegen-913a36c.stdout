program implicitinttointcastexample
implicit none
integer(4) :: integervalue1
integer(8) :: integervalue2
integervalue1 = 10
integervalue2 = int(integervalue1, kind=8)
print integervalue2 = int(integervalue1, kind=8)
, "Integer Value 1:", integervalue1
print print integervalue2 = int(integervalue1, kind=8)
, "Integer Value 1:", integervalue1
, "Integer Value 2 (Kind=8):",&
         integervalue2
end program implicitinttointcastexample
